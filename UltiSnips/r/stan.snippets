snippet stan "Sampling with stan, R code"
${1:model} <- stan_model(file = "${2:`!p snip.rv = snip.basename + '.stan'`}", model_name = "$1")
${3:sample} <- sampling($1, chains = ${4:Nchains}, iter = ${5:Niter}, data = ${6:dataframe})
${7:samps} <- extract($3, permuted = F, inc_warmup = T)
$0
endsnippet
